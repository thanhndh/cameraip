					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="maxNumOfFrames">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:int">
							<xs:attribute name="type" default="int"/>
